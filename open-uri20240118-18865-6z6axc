<script>

function formatAMPM(date) {
  var hours = date.getHours();
  var minutes = date.getMinutes();
  var ampm = hours >= 12 ? 'pm' : 'am';
  hours = hours % 12;
  hours = hours ? hours : 12; // the hour '0' should be '12'
  minutes = minutes < 10 ? '0'+minutes : minutes;
  var strTime = hours + ':' + minutes + ' ' + ampm;
  return strTime;
}
function scrollDown() {
jQuery('#chatcontent').animate({
	    		scrollTop: jQuery('#chatcontent')[0].scrollHeight}, "slow");
}

function addQuestion(){
var question = jQuery("#chatboxprompt").val();
if (question.length == 0) return;
 jQuery("#chatboxprompt").val("");
jQuery(".chatbox-popup__main").append("<div class=\"chat-text-container chat-text-question\">"+question+"<span class=\"time-right\">"+formatAMPM(new Date)+"</span></div>");
scrollDown();
jQuery.ajax({
  method: "POST",
  url: "/wp-content/plugins/web-chat/index.php",
  data: { action: 'webchat_action', appId: "7211ef4f-2b31-436f-a12b-248e71f17b3e", question: question },
  success: function (data) {
    // data contains the value returned by the server
    console.log("Return : " + data);
	var json = jQuery.parseJSON(data);
	console.log(json.answer);
    addAnswer(json.answer);
  }
});

}



function addAnswer(answer){


jQuery(".chatbox-popup__main").append("<div class=\"chat-text-container chat-text-answer\">"+answer+"<span class=\"time-right\">"+formatAMPM(new Date)+"</span></div>");
scrollDown();
}



jQuery( document ).ready(function() {
    console.log( "ready!" );



const chatbox = jQuery.noConflict();

chatbox(() => {

  chatbox(".chatboxenter").click(() =>
    addQuestion()
  );

  chatbox(".chatbox-open").click(() =>
    chatbox(".chatbox-popup, .chatbox-close").fadeIn()
  );

  chatbox(".chatbox-close").click(() =>
    chatbox(".chatbox-popup, .chatbox-close").fadeOut()
  );


chatbox(".chatbox-prompt").keypress(function(event) {
    var keycode = event.keyCode || event.which;
    if(keycode == '13') {
        addQuestion();
    }
});


});

});






</script>


